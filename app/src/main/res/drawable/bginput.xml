<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="30dp"/>
            <gradient
                android:startColor="#9ab2fd03"
                android:centerColor="#9bfad30c"
                android:endColor="#9457109e"/>

            <stroke
                android:color="#ffffff"
                android:width="2dp"/>
        </shape>
    </item>
</selector>